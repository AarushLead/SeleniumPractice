	<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>dark</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>

		<!-- title of the document -->
		<documentTitle>ExtentReport</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName> 
			<image id="image-zoom">
				<img src='D:\JAVAWORKSPACE\SeleniumPractice\images\download.png' />
			</image>
          Web Execution Report
		</reportName>

		<!-- location of charts in the test view -->
		<!-- top, bottom -->
		<testViewChartLocation>top</testViewChartLocation>

		<dateFormat>dd-MM-yyyy</dateFormat>

		<timeFormat>HH:mm:ss</timeFormat>
		<!-- custom javascript -->
		<scripts>
            <![CDATA[
                  $(document).ready(function() {
                  $("#image-zoom").src("src",src);
	              $("#image-zoom").append("src",src);
                   });
           ]]>
		</scripts>

		<!-- custom styles -->
		<styles>
            <![CDATA[
                .report-name { padding-left: 120px; } .report-name > img { float: left;height: 90%;margin-left: 120px;margin-top: 2px;width: auto; }
            ]]>
		</styles>
	</configuration>
</extentreports>